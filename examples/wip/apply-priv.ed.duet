let apply = sλ τ₁ : ☆,
               τ₂ : ☆,
               p  : priv
               .
               f : ∀ . (τ₁@p) ⊸⋆ τ₂,
               x : τ₁
               ⇒
  f@[ . x]
in
pλ εt : ℝ⁺,
   δt : ℝ⁺
   .
   εv : ℝ⁺[⟨ε⟩],
   δv : ℝ⁺[⟨δ⟩],
   x  : ℝ
   ⇒
   return apply@[ℝ,ℝ,⟨εt,δt⟩] (pλ . x : ℝ ⇒ gauss[1.0,εv,δv] <x> { x }) x
