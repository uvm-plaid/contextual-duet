let apply = sλ τ₁ : ☆,
               τ₂ : ☆,
               p₁  : priv
               p₂  : priv
               .
               f : (∀ . (τ₁@p₁,τ₂@p₂) ⊸⋆ τ₂),
               x : τ₁
               ⇒
  f x
in 
pλ εt : ℝ⁺,
   δt : ℝ⁺
   .
   εv : ℝ⁺[ε],
   δv : ℝ⁺[δ],
   x  : ℝ
   ⇒
apply@[ℝ,ℝ,⟨εt,δt⟩] (pλ . x : ℝ ⇒ gauss[1.0,εv,δv] <x> { x }) x
