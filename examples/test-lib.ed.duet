           , δ′⋅⟨∞,∞⟩
           ]
        ℝ
   ⇒
   pλ a : ℝ
   ⇒
   r₁ ← myLaplaceOne a;
   r₂ ← myLaplace @ ℝ⁺[1.0] ℝ⁺[1.0] a;
   r₃ ← myGauss @ ℝ⁺[1.0] @ ℝ⁺[1.0] ℝ⁺[1.0] ℝ⁺[1.0] a;
