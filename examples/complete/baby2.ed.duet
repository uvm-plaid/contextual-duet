
-- primitive gauss : ∀ εₛ : ℝ⁺, δₛ : ℝ⁺, s : ℝ⁺ .
-- (w : ℝ⁺[εₛ])
-- ⊸ [w⋅0.0] (q : ℝ⁺[δₛ])
-- ⊸ [w⋅0.0, q⋅0.0] (s : ℝ⁺[s])
-- ⊸ [w⋅0.0, q⋅0.0, s⋅0.0] (c : ℝ ⋅ s)
-- ⊸⋆ [ w⋅⟨∞,∞⟩, q⋅⟨∞,∞⟩, s⋅⟨∞,∞⟩, c⋅⟨εₛ,δₛ⟩ ] ℝ

let main = ∀  ε   : ℝ⁺,
              δ   : ℝ⁺,
              .
          pλ  ε   : ℝ⁺[ε] ⋅ 1.0,
              δ   : ℝ⁺[δ] ⋅ 1.0,
              r   : ℝ     ⋅ 1.0
              ⇒
  result ← gauss @ ε @ δ @ ℝ⁺[1.0] ε δ ℝ⁺[1.0] r;
  return result
in main

