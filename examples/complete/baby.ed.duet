let main = ∀  ε   : ℝ⁺,
              δ   : ℝ⁺,
              m   : ℕ,
              n   : ℕ
              .
          pλ  ε   : ℝ⁺[ε],
              δ   : ℝ⁺[δ],
              df :  𝕄 [L∞,U | m , n↦ℝ]
              ⇒
  result ← gauss @ ε @ δ <df> ε <df> δ <df> (real₂ @ m (mrows @ m @ n @ ℝ df));
  return result
in main

