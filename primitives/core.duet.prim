primitive gauss : ∀ εₛ : ℝ⁺, δₛ : ℝ⁺, s : ℝ⁺ .
(w : ℝ⁺[εₛ])
⊸ [w⋅0.0] (q : ℝ⁺[δₛ])
⊸ [w⋅0.0, q⋅0.0] (s : ℝ⁺[s])
⊸ [w⋅0.0, q⋅0.0, s⋅0.0] (c : ℝ ⋅ s)
⊸⋆ [ w⋅⟨∞,∞⟩, q⋅⟨∞,∞⟩, c⋅⟨εₛ,δₛ⟩ ] ℝ ,

primitive mgauss : ∀ εₛ : ℝ⁺, δₛ : ℝ⁺,m: ℕ, n: ℕ .
(w : ℝ⁺[εₛ])
⊸ [w⋅0.0] (q : ℝ⁺[δₛ])
⊸ [w⋅0.0, q⋅0.0] (c : 𝕄 [L∞, U|m, n↦ℝ ] ⋅ 1.0)
⊸⋆ [ w⋅⟨∞,∞⟩, q⋅⟨∞,∞⟩, c⋅⟨εₛ,δₛ⟩ ] 𝕄 [L∞, U|m, n↦ℝ ] ,


primitive mmapcol : ∀ τ₁: ☆,τ₂: ☆,m: ℕ, n: ℕ,q: ℕ, r: ℕ,s: ℕ .
(a : 𝕄 [L∞, U|m, n↦τ₁ ])
⊸ [a⋅q] (b : (c : τ₁) ⊸ [c⋅r] 𝕄 [L∞, U|m, n↦τ₂ ])
⊸ [ b⋅s ] 𝕄 [L∞, U|1.0, n↦τ₂ ] ,


primitive mmapcol₂ : ∀ τ₁: ☆,τ₂: ☆,m: ℕ, n: ℕ,q: ℕ, r: ℕ,s: ℕ .
(a : 𝕄 [L∞, U|m, n↦τ₁ ])
⊸ [a⋅q] (a₁ : 𝕄 [L∞, U|m, n↦τ₁ ])
⊸ [a₁⋅q] (b : (c : τ₁) ⊸ [c⋅r] 𝕄 [L∞, U|m, n↦τ₂ ])
⊸ [ b⋅s ] 𝕄 [L∞, U|1.0, n↦τ₂ ] ,

primitive mmaprow : ∀ τ₁: ☆,τ₂: ☆,m: ℕ, n: ℕ,q: ℕ, r: ℕ,s: ℕ .
(a : 𝕄 [L∞, U|m, n↦τ₁ ])
⊸ [a⋅q] (b : (c : τ₁) ⊸ [c⋅r] 𝕄 [L∞, U|1.0, n↦τ₂ ])
⊸ [ b⋅s ] 𝕄 [L∞, U|m, 1.0↦τ₂ ] ,

