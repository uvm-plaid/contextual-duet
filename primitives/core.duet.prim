primitive mclip :
∀ m: ℕ, n: ℕ, τ: ☆.
(x : 𝕄 [L∞, U|m, n↦τ ]) ⊸ [x⋅1.0] 𝕄 [L∞, L2|m, n↦τ ],

primitive aloop :
  ∀ ε: ℝ⁺, δ: ℝ⁺, δ′: ℝ⁺, k: ℕ, n: ℕ,
  Γ₁: cxt, Γ₂: cxt .
  (f : (z : 𝕄 [L∞,U| 1.0,n↦𝔻] ⋅ 1.0)
  ⊸⋆[ [Γ₁]⋅⟨ε,δ⟩
    , [Γ₂]⋅⟨∞,∞⟩
    , z⋅⟨∞,∞⟩
   ] 𝕄 [L∞,U|1.0,n↦ℝ])
  ⊸  [f⋅0.0] (x : 𝕄 [L1,U|1.0,n↦ℝ])
  ⊸  [f⋅0.0, x⋅0.0] (δ′′ : ℝ⁺[δ′] ⋅ 1.0)
  ⊸⋆ [ [Γ₁]⋅⟨2.0⋅ε⋅(√ (2.0⋅k⋅(㏒ (1.0/δ′)))),k⋅δ+δ′⟩
     , [Γ₂]⋅⟨∞,∞⟩
     , f⋅⟨∞,∞⟩
     , x⋅⟨∞,∞⟩
     , δ′′⋅⟨∞,∞⟩
     ] ℝ
