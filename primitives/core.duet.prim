(a : 𝕄 [L∞, U|m, n↦τ₁ ])
⊸ [a⋅q] (b : (c : τ₁) ⊸ [c⋅r] τ₂)
⊸ [ b⋅s ] τ₂ ,

primitive mmap : ∀ τ₁: ☆,τ₂: ☆,m: ℕ,n: ℕ .
(a : 𝕄 [L∞, U|m, n↦τ₁ ])
⊸ [a⋅1.0] (b : (c : τ₁) ⊸ [c⋅1.0] τ₂)
⊸ [ b⋅1.0 ] 𝕄 [L∞, U|m, n↦τ₂ ] ,

primitive mmap2 : ∀ τ₁: ☆,τ₂: ☆,m: ℕ, n: ℕ, p: ℕ,q: ℕ, r: ℕ,s: ℕ,t: ℕ .
(a₁ : 𝕄 [L∞, U|m, n↦τ₁ ])
⊸ [a₁⋅p] (a₂ : 𝕄 [L∞, U|m, n↦τ₁ ])
⊸ [a₂⋅q] (b : (c : τ₁) ⊸ [c⋅r] (d : τ₁) ⊸ [d⋅s] τ₂)
⊸ [ b⋅t ] 𝕄 [L∞, U|m, n↦τ₂ ] ,

primitive mfilter : ∀ τ₁: ☆,m: ℕ,n: ℕ .
(a : 𝕄 [L∞, U|m, n↦τ₁ ])
⊸ [a⋅1.0] (b : (c : τ₁) ⊸ [c⋅1.0] 𝔹)
⊸ [ b⋅1.0 ] 𝕄 [L∞, U|★, n↦τ₁ ] ,

primitive mLipGrad : ∀ m: ℕ, n: ℕ.
