primitive sample :
   ∀ ε: ℝ⁺, δ: ℝ⁺, rows₁: ℕ, rows₂: ℕ,
   n: ℕ, τ: ☆ .
   (a : ℕ[rows₁])
   ⊸  [a⋅0.0] (f : (xs' : 𝕄 [L∞,U|rows₁ ,n↦𝔻] ⋅ 1.0)
   ⊸⋆ ø (ys' : 𝕄 [L∞,U|rows₁,1.0↦𝔻] ⋅ 1.0)
   ⊸⋆ [xs'⋅⟨ε,δ⟩,ys'⋅⟨ε,δ⟩] 𝕄 [L∞,U|1.0,n↦ℝ])
   ⊸  [a⋅0.0,f⋅1.0] (xs : 𝕄 [L∞,L2|rows₂,n↦𝔻])
   ⊸  [a⋅0.0, xs⋅0.0,f⋅1.0] (ys : 𝕄 [L∞,U|rows₂,1.0↦𝔻] ⋅ 1.0)
   ⊸⋆ [xs⋅⟨2.0⋅(rows₁/rows₂)⋅ε,(rows₁/rows₂)⋅δ⟩,
       ys⋅⟨2.0⋅(rows₁/rows₂)⋅ε,(rows₁/rows₂)⋅δ⟩] τ,

primitive pfld-rows :
    ∀ ε: ℝ⁺, δ: ℝ⁺, m: ℕ, n: ℕ,rows: ℕ,τ: ☆ .
    (a : ℕ[rows])
    ⊸  [a⋅0.0] (θ : 𝕄 [L1,U|m,n↦ℝ])
    ⊸  [a⋅0.0, θ⋅0.0] (xs : 𝕄 [L1,U|m,n↦ℝ])
    ⊸  [a⋅0.0, xs⋅0.0, θ⋅0.0] (ys : 𝕄 [L1,U|m,1.0↦ℝ])
    ⊸  [a⋅0.0, xs⋅0.0, θ⋅0.0, ys⋅0.0] (f : (xs' : 𝕄 [L∞, L2|b, n↦𝔻 ] ⋅ 1.0)
    ⊸⋆ [xs⋅⟨ε,δ⟩] (ys' : 𝕄 [L∞, U|b, 1.0 ↦𝔻 ] ⋅ 1.0)
    ⊸⋆ [xs⋅⟨ε,δ⟩] (θ' : 𝕄 [L∞, U|1.0, n↦ℝ ] ⋅ 1.0)
    ⊸⋆ [xs⋅⟨ε,δ⟩,ys⋅⟨ε,δ⟩] 𝕄 [L∞, U|1.0, n↦ℝ ] ⋅ 1.0)
    ⊸⋆ [xs⋅⟨ε,δ⟩,
        ys⋅⟨ε,δ⟩] 𝕄 [L1,U|m,n↦ℝ],

