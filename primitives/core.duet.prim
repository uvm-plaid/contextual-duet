    ⊸  [f⋅1.0] (θ : 𝕄 [L∞,U|1.0,n↦𝔻])
    ⊸  [θ⋅0.0] (xs : 𝕄 [L∞,L2|m,n↦𝔻])
    ⊸  [xs⋅0.0] (ys : 𝕄 [L∞,U|m,1.0↦𝔻] ⋅ 1.0)
    ⊸⋆ [xs⋅⟨ρ⟩,ys⋅⟨ρ⟩] 𝕄 [L1,U|m,n↦ℝ],

